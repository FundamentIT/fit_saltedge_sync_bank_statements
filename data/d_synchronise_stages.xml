<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_initial">
        <field name="synchronise_stage_name">Initial</field>
        <field name="synchronise_stage_id">initial</field>
        <field name="fold" eval="False"/>
        <field name="synchronise_stage_sequence">5</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_start">
        <field name="synchronise_stage_name">Start</field>
        <field name="synchronise_stage_id">start</field>
        <field name="fold" eval="False"/>
        <field name="synchronise_stage_sequence">10</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_connecting">
        <field name="synchronise_stage_name">Connecting</field>
        <field name="synchronise_stage_id">connect</field>
        <field name="fold" eval="True"/>
        <field name="synchronise_stage_sequence">20</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_interactive">
        <field name="synchronise_stage_name">Interactive</field>
        <field name="synchronise_stage_id">interactive</field>
        <field name="fold" eval="True"/>
        <field name="synchronise_stage_sequence">30</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_success">
        <field name="synchronise_stage_name">Success</field>
        <field name="synchronise_stage_id">success</field>
        <field name="fold" eval="False"/>
        <field name="synchronise_stage_sequence">40</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_error">
        <field name="synchronise_stage_name">Error</field>
        <field name="synchronise_stage_id">error</field>
        <field name="fold" eval="False"/>
        <field name="synchronise_stage_sequence">50</field>
    </record>
    <record model="fit.saltedge.synchronise.stage" id="sync_stage_done">
        <field name="synchronise_stage_name">Done</field>
        <field name="synchronise_stage_id">done</field>
        <field name="fold" eval="False"/>
        <field name="synchronise_stage_sequence">60</field>
    </record>
</odoo>